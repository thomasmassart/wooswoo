<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns = "http://www.w3.org/ns/widgets"
xmlns:gap = "http://phonegap.com/ns/1.0"
id="be.movify.shopiready.sushiline"
version="1.0">
	<name>SUSHILINE</name>
	<description>Commandez vos sushi online</description>
	<author email="thomassart@gmail.com" href="#">Developer Team</author>
	<content src="index.html" />
	<access origin="*" />
	<preference name="fullscreen" value="true" />
	<preference name="UIWebViewBounce" value="false" />
	<preference name="webviewbounce" value="false" />
	<preference name="orientation" value="portrait" />
	<preference name="detect-data-types" value="false" />
	<preference name="phonegap-version" value="3.1.0" />
	<preference name="ios-statusbarstyle" value="black-opaque" />
	<preference name="target-device" value="handset" />
	<preference name="AutoHideSplashScreen" value="false" />
	
	<access origin="http://www.shopiready.com" subdomains="true" />
	<gap:platform name="ios" />
	
	<icon src="images/icon_57.png" gap:platform="ios" width="57" height="57"/>
	<icon src="images/icon_72.png" gap:platform="ios" width="72" height="72"/>
	<icon src="images/icon_76.png" gap:platform="ios" width="76" height="76"/>
	<icon src="images/icon_114.png" gap:platform="ios" width="114" height="114"/>
	<icon src="images/icon_120.png" gap:platform="ios" width="120" height="120"/>
	<icon src="images/icon_152.png" gap:platform="ios" width="152" height="152"/>
	<gap:splash src="images/splash.png" gap:platform="ios" width="320" height="480" />
	<gap:splash src="images/splash_2x.png" gap:platform="ios" width="640" height="960" />
	<gap:splash src="images/splash_iphone5.png" gap:platform="ios" width="640" height="1136" />
</widget>
